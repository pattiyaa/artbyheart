<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
  <%= link_to  t('session.register'), new_registration_path(resource_name) %><br />
<% end -%>


<%- if devise_mapping.confirmable? && controller_name != 'confirmations' %>
  <%= link_to t('session.confirm') , new_confirmation_path(resource_name) %><br />
<% end -%>