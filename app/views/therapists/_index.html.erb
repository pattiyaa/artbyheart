<div class="">
	<section id="Therapist" class="container special">
	<header>
		<h2><%= t('therapist.title')%></h2>
		<p><%= t('therapist.subtitle')%></p>
	</header>
	<div class="card-deck-wrapper">
		<div class="card-deck">
			<% authors.each do |author| %>
			 <div class="card col-xs-12 col-sm-6 col-md-3">
			    <%= link_to therapist_path(author), :class => "image featured"  do%>
	            <img class="card-img-top img-fluid"  src=<%= author.avatarimg.url%> alt="<%= author.name%>" />
	            <%end%>
	            <div class="card-block">
	            	<%= link_to therapist_path(author) do%>
				    <h3 class="card-title"><%= author.name%></h3>
				    <%end%>
				    <strong class="card-text"><%= author.title %></strong>
				    <p class="card-text"><small class="text-muted"><%= author.subtitle %></small></p>
				</div>
	          </div>
	        <%end%>
	         <div class="card col-xs-12 col-sm-6 col-md-3">
			    <%= link_to "#", :class => "image featured"  do%>
	            <img class="card-img-top img-fluid"  src="/images/artist.jpg" alt="be a part of our team" />
	            <%end%>
	            <div class="card-block">
				    <strong class="card-text"><%= t('therapist.join')%></strong>
				    <p class="card-text"><small class="text-muted"><%= t('therapist.join_desc')%></small></p>
				</div>
	          </div>
	    </div>
	</div>
	</section>
</div>