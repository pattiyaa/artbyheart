<%= simple_form_for(@workshop) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :title %>
    <%= f.input :desctiption %>
    <%= f.input :slung %>
    <%= f.input :meta_description %>
    <%= f.input :meta_keyworlds %>
    <%= f.input :meta_title %>
    <%= f.input :start %>
    <%= f.input :finish %>
    <%= f.input :capacity %>
    <%= f.association :teacher %>
    <%= f.input :available_on %>
    <%= f.input :discontinue_on %>
    <%= f.association :category %>
    <%= f.input :price %>
    <%= f.input :cost %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
